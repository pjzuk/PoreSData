FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nPlus;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -3 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    bottomAndTop
    {
        type            empty;
    }
    sides
    {
        type            empty;
    }
    plateOne
    {
        type            fixedFlux;
	n		nPlus;
	charge		1;
	D		1.337e-09;
	mu		5.2065343e-08;
	gradient	uniform 0.0;
	value		calculated;
    }
    plateTwo
    {
        type            fixedValue;
	value		uniform 1;
    }
}

