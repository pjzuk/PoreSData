FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      ePhi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
dimensions      [1 2 -3 0 0 -1 0];

internalField   uniform 0;

boundaryField
{
    plateOne
    {
          type            fixedValue;
          value           uniform -0.01;
    }

    plateTwo
    {
          type            fixedValue;
          value           uniform  0;
    }

    bottomAndTop
    {
        type            empty;
    }
    sides
    {
        type            empty;
    }
}

